on = 1
off = 0
motor1=0
motor2=1
in1 = 14

def motorpower(motor, power):
    if motor == motor1: 
        if power == on:
            GPIO.output(in1, GPIO.HIGH)
        if power == off:
            GPIO.output(in1, GPIO.LOW)
    if motor == motor2:
        if power == on:
            GPIO.output(in2, GPIO.HIGH)
        if power == off:
            GPIO.output(in2, GPIO.LOW)

